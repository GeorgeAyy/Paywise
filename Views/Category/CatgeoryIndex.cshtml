@model IEnumerable<MyMvcApp.Models.Category>

<h1>Categories</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="Category" asp-action="Create">Add New Category</a>
